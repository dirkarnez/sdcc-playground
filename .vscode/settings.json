{
    "files.associations": {
        ".eideignore": "ignore",
        "*.a51": "a51",
        "*.H": "c",
        "*.C": "c",
        "*.h": "c",
        "*.c": "c",
        "*.hxx": "cpp",
        "*.hpp": "cpp",
        "*.c++": "cpp",
        "*.cpp": "cpp",
        "*.cxx": "cpp",
        "*.cc": "cpp"
    },
    "files.autoGuessEncoding": true,
    "EIDE.ARM.Option.AutoGenerateRTE_Components": false,
    "C_Cpp.default.configurationProvider": "cl.eide",
    "C_Cpp.errorSquiggles": "disabled",
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.autoIndent": "advanced"
    }
}